{
  "author": "Tony Quetano",
  "babel": {
    "plugins": [
      "babel-plugin-macros"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "bugs": {
    "url": "https://github.com/planttheidea/inline-loops.macro/issues"
  },
  "dependencies": {
    "@babel/types": "^7.0.0",
    "babel-plugin-macros": "^2.0.0"
  },
  "description": "Macros that will inline loops for arrays an objects",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-tester": "^6.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "fast-equals": "^2.0.0",
    "jest": "^24.8.0"
  },
  "homepage": "https://github.com/planttheidea/inline-loops.macro#readme",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "/__runtime__/"
    ]
  },
  "keywords": [
    "babel-macros",
    "babel-plugin-macros",
    "babel",
    "inline",
    "loops",
    "inline-loops"
  ],
  "license": "MIT",
  "main": "build/inline-loops.macro.js",
  "name": "inline-loops.macro",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/inline-loops.macro.git"
  },
  "scripts": {
    "build": "babel inline-loops.macro.js -d build && cp ./index.d.ts ./build/",
    "dev": "npm run build && NODE_ENV=development webpack-dev-server --colors --progress --config=webpack/webpack.config.js",
    "lint": "eslint ./inline-loops.macro.js",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "typings": "./index.d.ts",
  "version": "1.0.0"
}
