{
  "author": "Tony Quetano",
  "babel": {
    "plugins": [
      "babel-plugin-macros",
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": false,
          "useESModules": false
        }
      ]
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "bugs": {
    "url": "https://github.com/planttheidea/inline-loops.macro/issues"
  },
  "dependencies": {
    "@babel/types": "^7.23.6",
    "babel-plugin-macros": "^3.1.0"
  },
  "description": "Macros that will inline loops for arrays and objects",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-external-helpers": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "babel-plugin-tester": "^11.0.4",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "fast-equals": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "homepage": "https://github.com/planttheidea/inline-loops.macro#readme",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "/__runtime__/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "keywords": [
    "babel-macros",
    "babel-plugin-macros",
    "babel",
    "inline",
    "loops",
    "inline-loops"
  ],
  "license": "MIT",
  "main": "dist/inline-loops.macro.js",
  "name": "inline-loops.macro",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/inline-loops.macro.git"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "copy:types": "cp ./index.d.ts ./dist/",
    "dist": "npm run build && npm run copy:types",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run lint && npm run dist && npm run test && npm run dist",
    "release": "release-it",
    "release:beta": "release-it --config=.release-it.beta.json",
    "test": "BABEL_DISABLE_CACHE=1 jest",
    "test:watch": "npm run test -- --watch"
  },
  "typings": "./index.d.ts",
  "version": "1.2.2"
}
