{
  "author": "Tony Quetano",
  "babel": {
    "plugins": [
      "babel-plugin-macros"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "bugs": {
    "url": "https://github.com/planttheidea/inline-loops.macro/issues"
  },
  "dependencies": {
    "@babel/types": "^7.0.0",
    "babel-plugin-macros": "^2.0.0"
  },
  "description": "Macros that will inline loops for arrays and objects",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/jest": "^24.0.13",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-tester": "^6.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "fast-equals": "^2.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2"
  },
  "homepage": "https://github.com/planttheidea/inline-loops.macro#readme",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/",
      "/__runtime__/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "keywords": [
    "babel-macros",
    "babel-plugin-macros",
    "babel",
    "inline",
    "loops",
    "inline-loops"
  ],
  "license": "MIT",
  "main": "dist/inline-loops.macro.js",
  "name": "inline-loops.macro",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/inline-loops.macro.git"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "copy:types": "cp ./index.d.ts ./dist/",
    "dist": "npm run build && npm run copy:types",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run lint && npm run dist && npm run test",
    "release": "release-it",
    "release:beta": "release-it --config=.release-it.beta.json",
    "test": "jest",
    "test:watch": "npm run dist && npm run test -- --watch"
  },
  "typings": "./index.d.ts",
  "version": "1.0.1"
}
